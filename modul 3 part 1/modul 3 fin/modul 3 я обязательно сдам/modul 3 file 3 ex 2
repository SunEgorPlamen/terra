def short_words(words):
    return [word for word in words if len(word) <= 5]
words = ["Волчий", "вой", "да", "лай", "собак", "Крепко", "до", "боли", "сжатый", "кулак", "Птицей", "стучится", "в", "жилах", "кровь", "Вера", "да", "надежда", любовь"] 
filtered_words = short_words(words)
print(filtered_words)